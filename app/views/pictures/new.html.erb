<h1>Upload Your Picture</h1>
  <%= form_for @picture do |f| %>
    <%=f.label :post_title%>:
    <%= f.text_field :title %><br>
    <%=f.label :image_url%>
    <%= f.text_field :image_url %><br>
    <%=f.label :user_id%>
    <%= f.text_field :user_id %><br>
    <%= f.submit value="Upload Picture" %>
  <% end %>